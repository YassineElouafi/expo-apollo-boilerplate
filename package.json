{
  "name": "expo-apollo-boilerplate",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "test": "jest --watch --coverage=false --changedSince=origin/master",
    "test:debug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "eject": "expo eject"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.16",
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-components": "^3.1.0",
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-testing": "^3.1.2",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/runtime": "^7.7.0",
    "apollo-boost": "^0.1.27",
    "apollo-cache-inmemory": "^1.3.0",
    "apollo-client": "^2.6.4",
    "apollo-link-context": "^1.0.18",
    "apollo-link-dedup": "^1.0.20",
    "babel-core": "6.26.3",
    "expo": "^36.0.0",
    "expo-constants": "~8.0.0",
    "expo-localization": "~8.0.0",
    "expo-permissions": "~8.0.0",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "i18n-js": "^3.3.0",
    "metro-react-native-babel-preset": "0.54.1",
    "moment": "^2.24.0",
    "react": "16.12.0",
    "react-base": "^0.2.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-content-loader": "^0.1.6",
    "react-native-dotenv": "0.2.0",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-web": "^0.11.7",
    "react-navigation": "4.0.10",
    "react-navigation-stack": "^1.10.3",
    "upgrade": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@types/react": "16.3.10",
    "@types/react-dom": "16.0.5",
    "@types/react-native": "0.55.2",
    "babel-jest": "^24.9.0",
    "babel-preset-expo": "5.2.0",
    "concurrently": "3.5.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "jest-expo": "^36.0.0",
    "react-native-scripts": "1.13.1",
    "react-test-renderer": "16.3.0-alpha.1",
    "rimraf": "2.6.2"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-cookies)/)"
    ]
  }
}
